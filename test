# Define a Function for adding of two numbers

def SubOp(a,b):
    c=a-b
    return c

# main program
print("Type of SubOp=", type(SubOp))

#Get two values from keyboard
a=float(input("Enter First value: "))
b=float(input("Enter Second Value: "))
res= SubOp(a,b)
print("Sum({},{})= {}".format(a,b,res))
